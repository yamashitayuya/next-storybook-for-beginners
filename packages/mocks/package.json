{
  "name": "mocks",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@stoplight/prism-cli": "^5.8.1",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
